** raise send Signal to self

#+BEGIN_SRC C :noweb yes
  #include <unistd.h>
  #include <stdio.h>
  #include <stdlib.h>
  #include <signal.h>

  int main()
  {
  	
	raise(SIGKILL); <</>> 
	return 0;
  }
#+END_SRC


